{
  "name": "recipes",
  "plural": "recipes",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mongodb": {
    "collection": "recipes"
  },
  "settings": {
    "mongodb": {
      "allowExtendedOperators": true
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "string"
    },
    "mainIngredient": {
      "type": "string"
    },
    "persons": {
      "type": "number"
    },
    "notes": {
      "type": "string"
    },
    "ingredients": {
      "type": [
        "string"
      ],
      "required": true
    },
    "preparation": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
